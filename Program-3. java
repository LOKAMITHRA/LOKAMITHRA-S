import java.util.*;
public class Program_3
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		int a;
		System.out.print("Enter a number : ");
		a=sc.nextInt();
		if(a%2==0) --a;
	    int series=1;
	    for(int i=1;i<a;i++)
	    {
            System.out.print(series+", ");
            series+=2;
        }
        System.out.print(series);
	}
}
